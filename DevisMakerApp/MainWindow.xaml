<Window x:Class="Additionneur.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Additionneur"
        xmlns:controls="clr-namespace:Additionneur.UserControls.Logging"
        mc:Ignorable="d"
        Title="MainWindow" MinWidth="720" MinHeight="480" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" WindowStyle="ThreeDBorderWindow">
    <Frame x:Name="MainWindowFrame" Grid.RowSpan="2" Content="{Binding ActiveView}" IsTabStop="False" NavigationUIVisibility="Hidden" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"/>
</Window>
